Var
// Seção de Declarações das variáveis 
      Numero, multiplo1, multiplo2: real


Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
     numero <- 0
     multiplo1 <- 0
     multiplo2 <- 0
     
     escreva (" Digite um numero: ")
     leia (numero)
     
     multiplo1 <- (numero) *2
     multiplo2 <- (numero) *3
     
     escreva ("Os multiplos do seu numero sao: ", multiplo1, multiplo2)
     
     

Fimalgoritmo
